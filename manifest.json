{
  "manifest_version": 3,
  "name": "Impatience",
  "version": "0.1.2",
  "description": "Playback speed control for HTML5 videos.",
  "action": {
       "default_popup": "popup.html",
       "default_title": "Impatience"
   },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "scripts/page.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
